title: Election Simulation
set-jump: post_election


- @1928: 1928 historical scenario
- @1930: 1930 historical scenario
- @1932: 1932 (July) historical scenario

@1928
on-arrival: {!
Q.workers = 17; // industry and trade
Q.old_middle = 10; // "old mittelstand"
// 16.4 trade & commerce, 6.6 public and private services, 4.4 domestic help
Q.new_middle = 5; // "new mittelstand"
Q.rural = 52; // agriculture and forestry employment... but many of those are proletarians so...
// source: https://surface.syr.edu/cgi/viewcontent.cgi?filename=3&article=1016&context=books&type=additional page 43, table 3.3
Q.unemployed = 5;
Q.swedes = 11;

Q.classes = ['workers', 'old_middle', 'new_middle', 'rural', 'unemployed', 'swedes'];
Q.parties = ['sdp', 'agrarian_league', 'ke', 'kk', 'sfp', 'other'];

// Support for party by social group
// they don't have to sum to 100 since the electoral calculations will be normalized.
Q.workers_sdp = 80;
Q.workers_agrarian_league = 5;
Q.workers_ke = 5;
Q.workers_kk = 5;
Q.workers_sfp = 10;
Q.workers_other = 5;

Q.old_middle_sdp = 30;
Q.old_middle_agrarian_league = 30;
Q.old_middle_ke = 20;
Q.old_middle_kk = 20;
Q.old_middle_sfp = 10;
Q.old_middle_other = 5;

Q.new_middle_sdp = 40;
Q.new_middle_agrarian_league = 5;
Q.new_middle_ke = 15;
Q.new_middle_kk = 30;
Q.new_middle_sfp = 10;
Q.new_middle_other = 5;

Q.rural_sdp = 20;
Q.rural_agrarian_league = 65;
Q.rural_ke = 5;
Q.rural_kk = 10;
Q.rural_sfp = 5;
Q.rural_other = 5;

Q.unemployed_sdp = 70;
Q.unemployed_agrarian_league = 10;
Q.unemployed_ke = 5;
Q.unemployed_kk = 10;
Q.unemployed_sfp = 5;
Q.unemployed_other = 5;

Q.swedes_sdp = 10;
Q.swedes_agrarian_league = 5;
Q.swedes_ke = 10;
Q.swedes_kk = 1;
Q.swedes_sfp = 75;
Q.swedes_other = 5;
!}
go-to: election_algorithm
set-jump: post_election

